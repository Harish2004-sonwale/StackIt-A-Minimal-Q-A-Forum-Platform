{
  "name": "stackit",
  "version": "1.0.0",
  "description": "A minimal Q&A forum platform",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "nodemon server/server.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "build": "cd client && npm install && npm run build",
    "deploy": "npm run build && git push origin master",
    "postinstall": "cd client && npm install && npm run build",
    "cloud": "npm run build && ./start-cloud.sh",
    "start-cloud": "./start-cloud.sh"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "rate-limit": "^1.0.2",
    "xss-clean": "^0.1.3",
    "express-mongo-sanitize": "^2.2.0",
    "csurf": "^1.11.0",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.0",
    "express-winston": "^4.4.0",
    "morgan": "^1.10.0",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "husky": "^8.0.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/user-event": "^14.5.1"
  },
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.14.13"
  },
  "keywords": [
    "react",
    "nodejs",
    "express",
    "mongodb",
    "qa",
    "forum",
    "stackit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Harish2004-sonwale/StackIt-A-Minimal-Q-A-Forum-Platform.git"
  }
}
